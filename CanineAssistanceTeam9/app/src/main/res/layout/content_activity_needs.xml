<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity_needs"
    tools:showIn="@layout/activity_needs">

    <EditText
        android:id="@+id/editText3"
        android:layout_width="387dp"
        android:layout_height="106dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="MY NEEDS"
        android:textSize="70sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13" />

    <HorizontalScrollView
        android:layout_width="2731dp"
        android:layout_height="58dp"
        android:scrollbarSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.952">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical" />
    </HorizontalScrollView>

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/dogfood"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="323dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="348dp"
        android:layout_height="364dp"
        app:srcCompat="@drawable/dogplay"
        tools:layout_editor_absoluteX="688dp"
        tools:layout_editor_absoluteY="344dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="418dp"
        android:layout_height="353dp"
        app:srcCompat="@drawable/water"
        tools:layout_editor_absoluteX="1148dp"
        tools:layout_editor_absoluteY="355dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="454dp"
        android:layout_height="350dp"
        app:srcCompat="@drawable/dogsick"
        tools:layout_editor_absoluteX="1659dp"
        tools:layout_editor_absoluteY="355dp" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="441dp"
        android:layout_height="349dp"
        app:srcCompat="@drawable/download"
        tools:layout_editor_absoluteX="2239dp"
        tools:layout_editor_absoluteY="360dp" />

</androidx.constraintlayout.widget.ConstraintLayout>